<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainListingFragment">
    <fragment
        android:id="@+id/mainListingFragment"
        android:name="com.example.anchorbooks_rodrigojaviergarridodagle.ui.fragments.MainListingFragment"
        android:label="MainListingFragment">
        <action
            android:id="@+id/next_action"
            app:destination="@id/bookSavedFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>


    <fragment
        android:id="@+id/bookSavedFragment"
        android:name="com.example.anchorbooks_rodrigojaviergarridodagle.ui.fragments.BookSavedFragment"
        android:label="BookSavedFragment">
        <action
            android:id="@+id/next_action"
            app:destination="@id/mainListingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>